  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -c bkcn-staging",
    "start-mcl-staging": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -c mcl-staging",
    "start-dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve",
    "start-mcl": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -c mcl-dev",
    "build": "ng build",
    "build-bkcn-staging": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration=bkcn-staging && node ./amplify/postbuild.js --target=bkcn.staging",
    "build-bkcn-production": "node ./node_modules/@angular/cli/bin/ng build --configuration=bkcn-production && node ./amplify/postbuild.js --target=bkcn.prod",
    "build-mcl-staging": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration=mcl-staging && node ./amplify/postbuild.js --target=mcl.staging",
    "build-mcl-production": "node ./node_modules/@angular/cli/bin/ng build --configuration=mcl-production && node ./amplify/postbuild.js --target=mcl.prod",
    "test": "ng test --code-coverage",
    "lint": "npx eslint \"./src/app/**/*.ts\" && npx prettier --check \"src/app/**/*.{ts,html,css,scss}\"",
    "lint:fix": "npx eslint \"./src/app/**/*.ts\" --fix && npx prettier --write \"src/app/**/*.{ts,html,css,scss}\"",
    "postinstall": "node patch-for-webpack.js"
  },
